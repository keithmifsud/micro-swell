{
    "name": "swellphp/micro-swell",
    "description": "A very simple and non-intrusive php micro-framework based on top of Symfony components and other cool packages.",
    "keywords": ["php", "development", "workflow"],
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Mifsud",
            "email": "mifsud.k@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "symfony/http-foundation": "^3.1",
        "symfony/http-kernel": "^3.1",
        "symfony/event-dispatcher": "~2.8|^3.0",
        "symfony/routing": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "twig/twig": "~1.0",
        "symfony/twig-bridge": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Swell\\Micro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swell\\Micro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
