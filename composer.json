{
    "name": "swellphp/swell",
    "description": "A very simple and non-intrusive php framework based on top of Symfony components and other cool packages.",
    "keywords": ["php", "development", "workflow"],
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Mifsud",
            "email": "mifsud.k@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swell\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "replace": {
        "swellphp/swell": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1"
        }
    }
}
